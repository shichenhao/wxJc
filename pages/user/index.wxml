<view class="container">
  <view class='login userInfo'>
    <image src='/lib/images/logo.png'></image>
    <view class='b'>
      {{
        userInfo.name
      }}
      <view>
      {{
        userInfo.mobile
      }}</view>
    </view>
  </view>
  <view class='userMoney'>
    <view class='userMoneyRow'>
      <view class='b'>
        20
        <view>元</view>
      </view>
      我的余额
    </view>
    <navigator class='userMoneyRow' url="/pages/user-envelopes/envelopes">
      <view class='b'>
        20
        <view>个</view>
      </view>
      我的红包
    </navigator>
    <navigator class='userMoneyRow' url="/pages/user-coupon/coupon">
      <view class='b'>
        20
        <view>个</view>
      </view>
      优惠券
    </navigator>
  </view>
  <view class='userMenu'>
    <navigator url="/pages/order/index">
      <image src='/lib/images/user-icon1.png'></image>  
      我的订单
    </navigator>
    <navigator url="/pages/user-add-list/list">
      <image src='/lib/images/user-icon2.png'></image>  
      收货地址
    </navigator>
    <navigator url="/pages/collection/collection">
      <image src='/lib/images/user-icon3.png'></image>  
      我的收藏
    </navigator>
  </view>
  <view class='telephone'>
    <view class='b'>
      客服电话：400-000-0000
    </view>
  </view>
  <view class='logout'>
    退出登录
  </view>
</view>